<div class=detailframe>
<div class=detailheadline>
<table width=100% cellspacing=0 cellpadding=0><tr>
<td class=detailheadline align=left>%DETAILLABEL%</td>
<td class=detailheadline align=right>%DETAILGROUPFUNCTIONS(edit,cancel,save)%</td>
</table>
</div>
   <table class=detailframe border=1>
      <tr class=fline>
         <td class=fname  style="width:20%;">%name(label)%:</td>
         <td class=finput  style="width:30%;">%name(detail)%</td>
         <td class=fname  style="width:20%;">%location(label)%:</td>
         <td class=finput  style="width:30%;">%location(detail)%</td>
      </tr>
      <tr class=fline>
         <td class=fname>%systemid(label)%:</td>
         <td class=finput colspan=3>%systemid(detail)%</td>
      </tr>
      <tr>
         <td class=fname colspan=4>%info(label)%:<br>%info(detail)%</td>
      </tr>
   </table>
</div>


