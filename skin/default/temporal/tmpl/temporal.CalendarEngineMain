<script language="JavaScript" >
var W5Base=createConfig({ useUTF8:false, mode:'auth',transfer:'JSON' });
function startSpinner(){
   var opts = {
     lines: 12, // The number of lines to draw
     length: 7, // The length of each line
     width: 5, // The line thickness
     radius: 20, // The radius of the inner circle
     color: '#fff', // #rbg or #rrggbb
     speed: 1, // Rounds per second
     trail: 66, // Afterglow percentage
     shadow: true // Whether to render a shadow
   };
   var target = document.getElementById('spin')
   var spinner = new Spinner(opts).spin(target);
}
function endSpinner(){
   $('#spin').html("");
}

function doExpand(){
   $('#calendar').toggle();
   resizeLevel1();
}

function formatSeconds(sec){
   var s="";

   var days = parseInt( sec / 86400 );
   var hours = parseInt( sec / 3600 ) % 24;
   var minutes = parseInt( sec / 60 ) % 60;
   var seconds = sec % 60;
   if (days>0){
      s+=days+"d "
      if (hours>0){
         s+=hours+"h "
      }
      if (minutes>0){
         s+=minutes+"m "
      }
   }
   else{
      if (hours>0){
         s+=hours+"h "
      }
      if (minutes>0){
         s+=minutes+"m "
      }
      if (seconds>0){
         s+=seconds+"s "
      }
   }
   return(s);
}

function resizeLevel1(){
   console.log($('#exdatadiv').parent());
   var newexdata=$('#exdatadiv').parent().height();
   var h=$('#exdatahead').height();
   $('#exdatadiv').height(newexdata-h-5);
}

function resizeLevel0(){
   var h=$('#exdatahead').height();
   $('#exdatadiv').height(h);
   setTimeout(resizeLevel1,100);
}


function calculatePercentBar(range_s,range_e,ev_s,ev_e){
   //console.log("calculatePercentBar",range_s,range_e,ev_s,ev_e);
   var eoverlab=false;
   var soverlab=false;
   var p1,p2,p3,t,t1,t3;
   if (ev_s<range_s){
      soverlab=true;
      ev_s=range_s;
      t1=0;
   }
   if (ev_e>range_e){
      eoverlab=true;
      ev_e=range_e;
      t3=0;
   }
   if (soverlab && eoverlab){
      p1=0; p2=100; p3=0;
   }
   else{
      t=range_e-range_s;
      if (!eoverlab){
         t3=range_e-ev_e;
      }
      if (!soverlab){
         t1=ev_s-range_s;
      }
      //console.log("t:",t," t1:",t1," t2(",t-(t3+t1),") t3:",t3);
      p1=parseInt(t1*100/t);
      p3=parseInt(t3*100/t);
      p2=100-(p1+p3);
   }

   return([p1,p2,p3,soverlab,eoverlab]); 
}

function setDefaultExData(){
   $('#ExHead').html("");
   $('#ExData').html("");
}

%DATADETAILCODE%

$(function() { // document ready
   var initialLangCode = 'de';
   $('#MainStartup').fadeIn();
   var buttons="today prev,next refresh";
   if (newTimespanHandler){
      buttons+=" new";
   }
 
   $('#calendar').fullCalendar({
      now: '%NOWSTAMP%',
      timezone: '%TIMEZONE%',
      aspectRatio: 2.3,
      scrollTime: '00:00',
      locale: initialLangCode,
      schedulerLicenseKey: 'GPL-My-Project-Is-Open-Source',
      slotDuration: '01:00:00', 
      customButtons: {
          refresh: {
              text: 'refresh',
              click: function() {
                  $('#calendar').fullCalendar('refetchEvents');
              }
          },
          new: {
              text: 'new',
              click: function() {
                 newTimespanHandler();
              }
          }
      },
      header: {
         left: buttons,
         center: 'title',
         right: 'timelineDay,timeline5TenDay,timelineWeek,month'
      },
      //defaultView: 'timelineDay',
      defaultView: 'month',

      eventRender: function (event, element,view) {
         if (event.holiday!="1"){
            element.attr('href', 'javascript:void(0);');
            element.click(function() {
                showDataDetail(event);
            });
         }
         else{
            var dateString = event.start.format("YYYY-MM-DD");
            $(view.el[0]).find('.fc-day[data-date='+dateString+']')
                            .css('background-color',event.color);
            $(view.el[0]).find('.fc-widget-header[data-date^='+dateString+']')
                            .css('background-color',event.color);
         }
      },
      views: {
         timeline5TenDay: {
            type: 'timeline',
            duration: { days: 15 }
         }
      },
      resourceAreaWidth: '20%',
      loading: function (b) {
         if (b){
            //console.log("fifi evspinner start");
            startSpinner(); 
         }
         else{
            //console.log("fifi evspinner ende");
            endSpinner(); 
         }
      },
      eventAfterAllRender: function (view) {
         setDefaultExData();
         endSpinner(); 
      },
      resourceLabelText: 'Changes',
      eventMouseover:function( e, jsEvent, view ) {
         $('#detail').html(e.id);
      },
      eventSources:[
         {
            url: 'HolidayList',
            editable:false
         },
         {
            url: 'EventList',
            editable:false
         }
      ]
   });
   resizeLevel0();
   $(window).resize(resizeLevel0);
});

</script>
